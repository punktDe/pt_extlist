{
  "name": "punktde/pt_extlist",
  "type": "typo3-cms-extension",
  "description": "List generator for TYPO3 based on Extbase. See http://extlist.punkt.de for further details.",
  "keywords": [
    "TYPO3",
    "extension"
  ],
  "homepage": "http://punkt.de",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<8.0"
  },
  "require-dev": {
    "mikey179/vfsStream": "1.4.*@dev",
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "PunktDe\\PtExtlist\\": "Classes"
    },
    "classmap": [
      "Classes/"
    ]
  },
  "replace": {
    "pt_extlist": "self.version",
    "typo3-ter/pt_extlist": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
